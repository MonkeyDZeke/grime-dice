{
  # "extends": "airbnb",
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "semi": ["error", "never"],
    "no-console": 0,
    "import/extensions": 0,
    "no-use-before-define": [
      "error",
      {
        "functions": false
      }
    ]
  }
}
